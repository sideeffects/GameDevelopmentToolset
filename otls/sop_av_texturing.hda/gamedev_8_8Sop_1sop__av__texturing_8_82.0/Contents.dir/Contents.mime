python	[ 0	locks=0 ]	(	"import hou, os

node = hou.pwd()
geo = node.geometry()

cache = geo.attribValue(\"cache\")
TexturingDir = os.path.join(cache, \"11_Texturing\")
mtl = os.path.join(TexturingDir, \"texturedMesh.mtl\")

QuickMat = hou.node(\"../mesh_materials\")
# Quick Reset. Forces removal of all materials
QuickMat.parm(\"mMaterialEntries\").set(0)


if not os.path.isfile(mtl):
    hou.ui.displayMessage(\"No material generated!\", buttons=('OK',), severity=hou.severityType.Error, title=\"Error\")
else:

    with open(mtl, 'r') as f:
        lines = f.read().splitlines()
    f.close()

    Materials = {}
    CurMat = \"\"
    for line in lines:
        if line.startswith(\"#\"):
            pass
        else:
            data = filter(None, line.split(\" \"))
            if len(data) == 0:
                pass
            else:
                if data[0] == \"newmtl\":
                    CurMat = data[1]
                    Materials[CurMat] = {\"Material\": CurMat}
                elif data[0] == \"map_Kd\":
                    Materials[CurMat].update({\"DiffuseTex\":data[1]})
                elif data[0] == \"Kd\":
                    Materials[CurMat].update({\"DiffuseColor\": [float(data[1]), float(data[2]), float(data[3])]})
                elif data[0] == \"Ns\":
                    Materials[CurMat].update({\"Specular\": float(data[1])})
                elif data[0] == \"d\":
                    Materials[CurMat].update({\"Opacity\": float(data[1])})
                elif data[0] == \"Ni\":
                    Materials[CurMat].update({\"IOR\": float(data[1])})


    QuickMat.parm(\"mMaterialEntries\").set(len(Materials.keys()))

    for x, material in enumerate(Materials.keys()):
        QuickMat.parm(\"materialname_%s\" % (x+1)).set(Materials[material][\"Material\"])
        QuickMat.parm(\"groupselection_%s\" % (x+1)).set(\"@shop_materialpath=/mat/%s\" % Materials[material][\"Material\"])
        QuickMat.parm(\"basecolor_texture_%s\" % (x+1)).set(os.path.join(TexturingDir, Materials[material][\"DiffuseTex\"]))

    QuickMat.hm().OnMultiparmChange(QuickMat)

    hou.node(\"../mesh_geo\").cook(force=True)

hou.hscript(\"glcache -c;\") #refresh textures"	)
}

