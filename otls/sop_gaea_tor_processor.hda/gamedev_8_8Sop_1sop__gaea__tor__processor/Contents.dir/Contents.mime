Content-Disposition: attachment; filename="hdaroot/extract_inputmaps.parm"
Content-Type: text/plain

{
version 0.8
python	[ 0	locks=0 ]	(	"import os
node = hou.pwd()
geo = node.geometry()

# Add code to modify contents of geo.
# Use drop down menu to select examples.


ParmGroup = hou.pwd().parent().parmTemplateGroup()
ExportParmTemplates = ParmGroup.find(\"generated_houdiniinputfolder\")

geo.addAttrib(hou.attribType.Point, \"layer\", \"\", create_local_variable=False)
geo.addAttrib(hou.attribType.Point, \"texture\", \"\", create_local_variable=False)
geo.addAttrib(hou.attribType.Global, \"hasalltextures\", 1)

if ExportParmTemplates != None:

        ExportParmTemplates = ExportParmTemplates.parmTemplates()


        for parmtemplate in ExportParmTemplates:
            LayerToExtract = hou.pwd().parent().parm(parmtemplate.name()).evalAsString() 
            pt = geo.createPoint()
            pt.setAttribValue(\"layer\", LayerToExtract)
            TexturePath = (os.path.join(hou.pwd().parent().parm(\"sTempDir\").evalAsString(), \"read_%s_%s.png\" % (hou.pwd().parent().name(), LayerToExtract))).replace(\"\\\\\\\", \"/\")
            if os.path.isfile(TexturePath):
                pt.setAttribValue(\"texture\", TexturePath)
            else:
                geo.setGlobalAttribValue(\"hasalltextures\", 0)
else:
        geo.setGlobalAttribValue(\"hasalltextures\", 0)

    
    "	)
}

