{
version 0.8
python	[ 0	locks=0 ]	(	"node = hou.pwd()
geo = node.geometry()

# Add code to modify contents of geo.
# Use drop down menu to select examples.
HFHeightOutput = hou.node(\"../heightfield_output_height/\")
HFMaskOutput = hou.node(\"../heightfield_output_masks/\")

ParmGroup = hou.pwd().parent().parmTemplateGroup()
ExportParmTemplates = ParmGroup.find(\"generated_houdinioutputfolder\").parmTemplates()

for parmtemplate in ExportParmTemplates:
    LayerToExtract = hou.pwd().parent().parm(parmtemplate.name()).evalAsString()
    
    if LayerToExtract == \"height\":
        HFHeightOutput.parm(\"execute\").pressButton()
        
    else:
        HFMaskOutput.parm(\"red_channel\").set(LayerToExtract)
        HFMaskOutput.parm(\"execute\").pressButton()
        
        
    
"	)
}

